@model List<B2CAdminPortal.Models.OrderVM>


<div id="myModal" class="modal fade" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Modal Header</h4>
            </div>
            <div class="modal-bodys" id="modal-bodys">

                <table id="example2">
                    <thead>
                        <tr>
                            <th>Product Name</th>
                            <th>Discount %</th>
                            <th>Price</th>
                            <th>Quantity</th>
                            <th>Sub Total</th>
                            <th>Discounted Amount</th>
                            <th>Total</th>
                            <th>Ordered Date </th>
                        </tr>
                    </thead>

                    <tbody>

                        @if (Model != null && Model.Count > 0)
                        {
                            foreach (var item in Model)
                            {
                                <tr data-id="58">
                                    <td class="td-img text-left">
                                        <img src="@item.MasterImageUrl" alt="Add Product">
                                        <div class="items-dsc">
                                            <h5>
                                                @item.Name
                                            </h5>
                                        </div>
                                    </td>

                                    <td class="pricecol">
                                        @item.Discount
                                    </td>
                                    <td>
                                        <strong class="pricesymbol"> </strong> @item.Price
                                    </td>
                                    <td>
                                        @item.Quantity
                                    </td>
                                    <td>
                                        <strong class="pricesymbol"> </strong>    @item.SubTotalPrice
                                    </td>
                                    <td>
                                        <strong class="pricesymbol"> </strong>   @item.DiscountAmount
                                    </td>
                                    <td>
                                        <strong class="pricesymbol"> </strong> @item.TotalPrice
                                    </td>

                                    <td>
                                        @item.CreatedOn
                                    </td>
                                </tr>
                            }
                        }


                    </tbody>

                </table>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
